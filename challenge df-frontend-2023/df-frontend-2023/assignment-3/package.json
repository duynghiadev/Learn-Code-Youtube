		"@types/node": "^16.18.52",
		"@types/react": "^18.2.22",
		"@types/react-dom": "^18.2.7",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"prettier": "^3.0.3"
	},
	"lint-staged": {
		"*.{css,html,json,tsx,ts}": [
			"prettier --write ."
		],
		"*.{tsx,ts}": "eslint --fix"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
